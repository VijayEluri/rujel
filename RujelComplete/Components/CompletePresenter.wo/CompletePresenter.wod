Container : OnClickContainer {
  elementName = "td";
  disabled = disabled;
  title = dict.hover;
  onclick = onClick;
  align = "center";
  hide = currAddOn.hide;
  invokeAction = stamp;
}

Header : WOConditional {
  condition = student;
  negate = true;
}

Stamp : WOImage {
  framework = "RujelComplete";
  filename = "stamp.gif";
  height = 38;
  width = 38;
}

Stamp2 : WOImage {
  framework = "RujelComplete";
  filename = "stamp.gif";
  height = 24;
  width = 24;
}

Row : WOConditional {
  condition = student;
}

IsComplete : WOConditional {
  condition = dict.closed;
}

Incomplete : WOConditional {
  condition = dict.closed;
  negate = true;
}

Tick : WOCheckBox {
  checked = dict.closed;
  disabled = currAddOn.access._create;
  name = dict.id;
  title = dict.title;
  onchange = "if(returnField==null)returnField=new Object();returnField[name]=checked;";
}
