 ALTER TABLE AI_PROGNOSIS
  ADD COLUMN VALUE_STATE smallint NOT NULL default 0;

UPDATE AI_PROGNOSIS set VALUE_STATE = 1 where PR_MARK = 'н/а' or PR_MARK = '2';
